package dev.com.abstraction;

public abstract class AbstractExample {
	
	//AbstractExample a = new AbstractExample();  //cannot have objects for abstract class
	AbstractExample() { // we can have constructor inside an abstract class.
	System.out.println("constructor of abstract example clss");		
	}

	abstract void display();
	public void show() {
		System.out.println("concrete method of abstract class");
	}
	
}
